{
  "name": "linter-scalac",
  "main": "./lib/linter-scalac",
  "version": "1.2.0",
  "description": "Lint Scala on the fly, using scalac. Also possible to use other Scala linters, such as WartRemover, via compiler options.",
  "repository": "https://github.com/AtomLinter/linter-scalac",
  "license": "MIT",
  "engines": {
    "atom": ">0.90.0"
  },
  "dependencies": {
    "atom-linter": "^4.1.1",
    "atom-package-deps": "^3.0.6",
    "glob": "^6.0.1",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "promised-exec": "1.0.1",
    "rimraf": "^2.5.0"
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
